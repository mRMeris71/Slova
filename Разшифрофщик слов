meme_dict = {
            "КРИНЖ": "Что-то очень странное или стыдное",
            "ЛОЛ": "Что-то очень смешное"
            }
slovo = input('Введите непонятное слово(БОЛЬШИМИ БУКВАМИ!): ')
if slovo in meme_dict.keys():
    print (meme_dict[slovo])
else:
    Print('Такого слова нету в базе данных')
